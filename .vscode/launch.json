{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Nextjs: Server",
      "skipFiles": ["<node_internals>/**"],
      "port": 9229,
      "resolveSourceMapLocations": ["${workspaceFolder}/**", "!**/node_modules/**"]
    },
    {
      "type": "pwa-chrome",
      "request": "launch",
      "name": "Nextjs: Client",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}"
    }
  ]
}
